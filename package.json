{
  "name": "x402r-arbiter-eigencloud",
  "version": "0.1.0",
  "description": "Verifiable payment dispute arbiter on EigenCloud for x402r protocol",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "verify": "tsx src/verify-eigenai.ts",
    "cli": "tsx cli/bin/x402r.ts",
    "cli:e2e": "tsx cli/e2e-test.ts",
    "seed": "tsx -r dotenv/config court-seed.ts",
    "dashboard": "cd court-ui && npm run dev",
    "dashboard:build": "cd court-ui && npm run build"
  },
  "dependencies": {
    "@x402r/arbiter": "^0.0.2",
    "@x402r/core": "^0.0.2",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.0",
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^20.0.0",
    "@x402/core": "^2.3.0",
    "@x402/evm": "^2.3.0",
    "@x402r/client": "^0.0.2",
    "@x402r/evm": "file:../x402r-scheme/packages/evm",
    "@x402r/helpers": "^0.0.2",
    "@x402r/merchant": "^0.0.2",
    "tsx": "^4.19.0",
    "typescript": "^5.0.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "@x402/core@2.3.1": "patches/@x402__core@2.3.1.patch"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "MIT",
  "keywords": [
    "eigencloud",
    "x402r",
    "arbiter",
    "verifiable",
    "dispute-resolution"
  ]
}
